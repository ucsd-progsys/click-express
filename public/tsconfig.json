{
    "compilerOptions": {
        "target": "es5",
        "module": "amd",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": false,
        "out": "./js/clicker.js",
        "watch": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./js/clicker.ts"
    ]
}
